
public class Student {
	int studentRegNo;
	String Name;
	Date Doa;
	
	public Student(int studentRegNo,String Name, Date Doa ) {
		this.studentRegNo=studentRegNo;
		this.Name=Name;
		this.Doa=Doa;
	}
	public void display() {
		System.out.println(this.studentRegNo+" , "+this.Name+" , "+this.Doa);
		
		}
	
	
}
